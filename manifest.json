{
  "manifest_version": 3,
  "name": "rYTplugin",
  "description": "rYTplugin",
  "version": "1",
  "action": {
    "default_title": "rYTplugin",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "main.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}